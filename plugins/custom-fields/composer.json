{
  "name": "idg/editor-fields",
  "description": "Add custom fields to the post editor.",
  "type": "wordpress-client-muplugin",
  "version": "1.0.0-alpha.1",
  "require": {
    "swaggest/json-schema": "^0.12.29"
  },
  "require-dev": {
    "automattic/jetpack-autoloader": "^1"
  },
  "autoload": {
    "files": [
      "inc/asset-settings.php",
      "inc/utils.php",
      "inc/functions.php"
    ],
    "psr-4": {
      "Custom_Fields\\": "inc/"
    },
    "classmap": [ "inc/" ]
  },
  "scripts": {},
  "config": {
    "preferred-install": "dist"
  }
}
