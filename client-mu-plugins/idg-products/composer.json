{
  "name": "idg/products",
  "description": "The products plugin for IDG.",
  "type": "wordpress-client-muplugin",
  "version": "1.0.0-alpha.1",
  "require": {
    "mobiledetect/mobiledetectlib": "^2.8"
  },
  "require-dev": {
    "automattic/jetpack-autoloader": "^1"
  },
  "autoload": {
    "files": [
      "inc/asset-settings.php",
      "inc/utils.php",
      "inc/setup.php"
    ],
    "psr-4": {
      "IDG\\Products\\": "inc/"
    },
    "classmap": [ "inc/" ]
  },
  "scripts": {},
  "config": {
    "preferred-install": "dist"
  }
}
