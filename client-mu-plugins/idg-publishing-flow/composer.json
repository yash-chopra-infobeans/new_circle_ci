{
  "name": "idg-publishing-flow",
  "description": "The publishing flow and pipeline for all IDG websites.",
  "type": "wordpress-client-muplugin",
  "version": "1.0.0-alpha.1",
  "require": {
    "rakit/validation": "^1.2"
  },
  "require-dev": {
    "automattic/jetpack-autoloader": "^1"
  },
  "autoload": {
    "files": [
      "inc/setup.php",
      "inc/utils.php",
      "inc/cli.php",
      "inc/asset-settings.php"
    ],
    "psr-4": {
      "IDG\\": "inc/"
    },
    "classmap": [ "inc/" ]
  },
  "scripts": {},
  "config": {
      "preferred-install": "dist"
  }
}
