{
  "name": "idg-configuration",
  "description": "Global configuration for 3rd party plugins used on IDG websites.",
  "type": "wordpress-client-muplugin",
  "version": "1.0.0",
  "require": {},
  "require-dev": {
    "automattic/jetpack-autoloader": "^1"
  },
  "autoload": {
    "files": [
      "inc/workarounds.php",
      "inc/setup.php",
      "inc/utils.php",
      "inc/asset-settings.php"
    ],
    "psr-4": {
      "IDG\\": "inc/"
    },
    "classmap": [ "inc/" ]
  },
  "scripts": {},
  "config": {
      "preferred-install": "dist"
  }
}
